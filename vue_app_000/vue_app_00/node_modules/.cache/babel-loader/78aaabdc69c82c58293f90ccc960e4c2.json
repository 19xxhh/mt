4>\r\n            <div class=\"info\">{{item.price}}</div>\r\n            <mt-button @click=\"addCart\" :data-lid=\"item.lid\" :data-price=\"item.price\" :data-lname=\"item.lname\">加入购物车</mt-button>\r\n        </div>\r\n        <mt-button @click=\"jumpCart\">查看更多</mt-button>\r\n        <mt-button type=\"primary\" size=\"large\" @click=\"loadMore\">加载更多</mt-button>\r\n    </div>\r\n</template>\r\n<script>\r\nexport default {\r\n    data(){\r\n        return{\r\n            list:[],  //商品列表数组\r\n            pno:0\r\n        }\r\n    },\r\n    methods:{\r\n        jumpCart(){\r\n            this.router.push(\"/\")\r\n        },\r\n        addCart(e){\r\n            var lid=e.target.dataset.lid;\r\n            var n=e.target.dataset.lname;\r\n            var p=e.target.dataset.price;\r\n            console.log(lid+\":\"+n+\":\"+p)\r\n            // 发送ajax请求\r\n            var url=\"addcart\";\r\n            var obj={lid:lid,lname:n,price:p};\r\n            // 获取返回的结果\r\n            this.axios.get(url,{params:obj}).then(res=>{\r\n                 if(res.data.code==-1){\r\n                this.$toast(\"请先登录再购买商品\");\r\n                }else{\r\n                this.$toast(\"添加成功\");\r\n            }\r\n            })\r\n        },\r\n        loadMore(){\r\n            // 获取商品分页数据\r\n            // console.log(123);\r\n            // 发送请求\r\n            var url=\"product\";\r\n            this.pno++; //当前页面+1\r\n            var obj={pno:this.pno}; //创建参数对象\r\n            this.axios.get(url,{params:obj}).then(res=>{\r\n            // 获取服务器返回结果\r\n            console.log(res.data.data)\r\n            // 将返回结果保存\r\n            // this.list=res.data.data;\r\n            var rows=this.list.concat(res.data.data);\r\n            this.list=rows;\r\n            });//合并多页\r\n        }\r\n    },\r\n    created(){\r\n        this.loadMore();\r\n            console.log(\"1:created 组件创建成功\")\r\n        }\r\n}\r\n</script>\r\n<style scoped>\r\n    /* 最外层样式 */\r\n    .product-app{\r\n        display:flex;\r\n        flex-wrap:wrap;\r\n        justify-content:space-between;\r\n        padding:10px;\r\n    }\r\n    /* 修饰商品样式 */\r\n    .goods-item{\r\n        width:49%;\r\n        border:1px solid #ccc;\r\n        border-radius:5px;\r\n        margin:2px 0;\r\n        padding:2px;\r\n        box-sizing:border-box;\r\n        display:flex;\r\n        flex-direction:column;\r\n        min-height:147px;\r\n    }\r\n    /* 图片样式 */\r\n    .goods-item img{\r\n        width:100%;\r\n    }\r\n</style>\r\n"],"sourceRoot":"src\\components\\xz"}]}