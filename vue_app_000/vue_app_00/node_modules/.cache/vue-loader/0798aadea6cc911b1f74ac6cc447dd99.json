\n        <!-- 顶部按钮-全选 -->\r\n        <div class=\"selectAll\">\r\n            全选<input type=\"checkbox\">\r\n        </div>\r\n        <!-- 购物车商品信息 -->\r\n        <div class=\"cart-item\" v-for=\"(item,i) of list\" :key=\"i\">\r\n            <div class=\"leftImgText\">\r\n                <input type=\"checkbox\">\r\n                <img :src=\"'http://127.0.0.1:3000/'+item.img_url\" alt=\"\">\r\n                <div class=\"lname\">{{item.lname}}</div>\r\n                <div class=\"price\">{{item.price}}</div>\r\n            </div>\r\n            <mt-button @class=\"delItem\">删除</mt-button>\r\n        </div>\r\n        <!-- 按钮  删除选中商品 -->\r\n        <div>\r\n            <mt-button>删除选中商品</mt-button>\r\n            小计：<span></span>\r\n        </div>\r\n    </div>\r\n</template>\r\n<script>\r\nexport default {\r\n    data(){\r\n        return{\r\n            list:[]\r\n        }\r\n    },\r\n    methods:{\r\n        delItem(e){\r\n            // 功能：删除购物车中指定的商品\r\n            // 获取购物车id\r\n            var id=e.target.dataset.id;\r\n            // 交互提示用户是否删除指定商品\r\n            // 同意\r\n            // 发送ajax请求\r\n            // 返回服务器返回内容\r\n            // 重新调用loadMore()\r\n        },\r\n        loadMore(){\r\n            // 功能：加载购物车中的数据\r\n            // 当组件创建成功后，调用\r\n            console.log(123);\r\n            // 1 发送ajax请求\r\n            var url=\"cart\";\r\n            console.log(456)\r\n            this.axios.get(url).then(res=>{\r\n                console.log(res);\r\n            // 2 获取返回结果\r\n            if(res.data.code==-1){\r\n            // 3 如果用户没有执行登录操作  跳转到登录组件\r\n            this.$messagebox(\"消息\",\"请登录\").then(res=>{\r\n                this.$router.push(\"/Login\");\r\n                return;\r\n            });\r\n            // console.log(\"999\")\r\n            }else{\r\n            // 4 获取数据成功\r\n            var list=res.data.data;\r\n            this.list=list;\r\n            console.log(this.list);\r\n            }\r\n             })\r\n        }\r\n    },\r\n    created(){\r\n        this.loadMore();\r\n    }\r\n}\r\n</script>\r\n<style scoped>\r\n    /* 商品项目元素 */\r\n    .cart-item{\r\n        display:flex;\r\n        justify-content: space-between;\r\n        align-items: center;\r\n        border-bottom: 1px solid #ccc;\r\n        margin-top:5px;\r\n    }\r\n    /* 左侧图片与文字 */\r\n    .leftImgText{\r\n        display:flex;\r\n        align-items: center;\r\n        margin-left:5px;\r\n\r\n    }\r\n    /* 单独修改图片宽度和高度 */\r\n    .leftImgText img{\r\n        width:50px;\r\n        height:50px;\r\n        margin-left:10px;\r\n    }\r\n    /* 商品价格 */\r\n    .price{\r\n        color:red;\r\n        margin-left:25px;\r\n    }\r\n</style>\r\n"]}]}